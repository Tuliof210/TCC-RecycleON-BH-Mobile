{
  "expo": {
    "name": "RecycleON BH",
    "slug": "recycleon-bh",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "splash": {
      "image": "./src/assets/AppIcons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#87D459"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.recycleonbh.recycleonbh"
    },
    "android": {
      "package": "com.recycleonbh.recycleonbh",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/AppIcons/playstore.png",
        "backgroundColor": "#87D459"
      },
      "config": {
        "googleMaps": {
          "apiKey": "GOOGLE_MAPS_ANDROID_API_KEY"
        }
      }
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    }
  }
}
